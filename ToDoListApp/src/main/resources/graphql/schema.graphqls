type Query {
    getUserInfoById(userId: ID!): UserInfo
    getAllUsers: [UserInfo!]!

}

type Mutation {
    checkValidity(email: String!, password: String!): Boolean!
    registerUser(email: String!, password: String!, name:String!): String!
}

type UserInfo {
    userId: ID!
    name: String!
    password: String!
    role: Role!
}

enum Role {
    ROLE_USER
    ROLE_ADMIN
}
